{
  "name": "ts-type-tester",
  "version": "0.1.0",
  "description": "Test all your type-tests and incorporate into your CI-CD and deployment strategy",
  "scripts": {
    "build": "tsup src/ts-test.ts --format=esm -d bin --clean && chmod +x \"bin/ts-test.mjs\"",
    "test": "node ./bin/ts-test.mjs"
  },
  "bin": {
    "ts-test": "node ./bin/ts-test.mjs"
  },
  "keywords": [
    "typescript",
    "testing"
  ],
  "author": "Ken Snyder<ken@ken.net>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^7.0.1",
    "fast-glob": "^3.3.2",
    "inferred-types": "^0.42.1",
    "pathe": "^1.1.2",
    "typescript": "^5.4.2",
    "watcher": "^2.3.0"
  },
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/node": "18",
    "tsup": "^8.0.2",
    "vitest": "^1.4.0"
  }
}
